<h1>Listing Postings</h1>

<%= will_paginate @postings %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <% @postings.each do |posting| %>
    <tr>
      <td><%= posting.title %></td>
      <td><%= posting.location %></td>
      <td><%= link_to 'Show', posting %></td>
    </tr>
    <% end %>
  </table>

